{
  "name": "d3-template-plugin",
  "version": "1.3.0",
  "description": "Template plugin for D3js.",
  "keywords": [
    "d3",
    "d3-plugin",
    "d3-template",
    "template",
    "d3-module"
  ],
  "homepage": "https://github.com/ErikOnBike/d3-template.git",
  "license": "BSD-3-Clause",
  "author": {
    "name": "Erik Stel",
    "url": "https://www.codeparadise.nl"
  },
  "main": "build/d3-template.js",
  "jsnext:main": "index",
  "repository": {
    "type": "git",
    "url": "https://github.com/ErikOnBike/d3-template"
  },
  "scripts": {
    "pretest": "rm -rf build && mkdir build && rollup -c",
    "test": "tape 'test/**/*-test.js' && eslint index.js src test",
    "prepublish": "npm run test && uglifyjs build/d3-template.js -c -m -o build/d3-template.min.js"
  },
  "dependencies": {
    "d3-selection": "^1",
    "d3-transition": "^1"
  },
  "devDependencies": {
    "d3-color": "^1.0.3",
    "d3-interpolate": "^1.1.6",
    "eslint": "3",
    "jsdom": "11",
    "nyc": "^11.4.1",
    "rollup-plugin-node-resolve": "^3",
    "rollup-plugin-commonjs": "^8",
    "rollup": "^0",
    "tape": "4",
    "uglify-js": "^2.8.11",
    "node-es-module-loader": "^0.3"
  }
}
